{
  "name": "weipaitang/utils",
  "description": "Utils工具",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "illuminate/support": "5.*",
    "weipaitang/common-library": "~0.0.1",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "autoload": {
    "psr-4": {
