{
    "name": "pay/php-sdk",
    "description": "PayCenter PHP SDK",
    "type": "library",
    "license": "proprietary",
    "support": {
        "issues": "https://gitlab.weipaitang.com/pay/php-sdk/issues",
        "source": "https://gitlab.weipaitang.com/pay/php-sdk"
    },
    "require": {
        "php": "7.*",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*"
    },
    "suggest": {
        "vlucas/phpdotenv": "Required to use .env files."
    },
    "autoload": {
        "psr-4": {
            "PayCenter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PayCenter\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit tests"
        ]
    },
    "prefer-stable": true
}
